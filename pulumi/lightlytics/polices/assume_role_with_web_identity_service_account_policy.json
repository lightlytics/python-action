{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Federated": "PLACE_HOLDER_FOR_OIDC_ARN"
      },
      "Action": "sts:AssumeRoleWithWebIdentity",
      "Condition": {
        "StringEquals": {
          "PLACE_HOLDER_FOR_OIDC_URL_WITH_SUB_SUFFIX": "PLACE_HOLDER_FOR_OIDC_SERVICE_ACCOUNT_SUB"
        }
      }
    }
  ]
}