[neo4j]
{{neo4j_core_0}}
{{neo4j_core_1}}
{{neo4j_core_2}}
{{neo4j_replica_1}}
{{neo4j_replica_2}}

[neo4j:vars]
ansible_ssh_user=ubuntu
ansible_ssh_private_key_file=~/prod-keys/{{private_key}}
ansible_ssh_common_args= -o ProxyCommand="ssh -q ec2-user@{{bastion_host}} -i ~/{{private_key}} -W %h:%p"

[bastion]
{{bastion_host}}


[bastion:vars]
ansible_ssh_user=ec2-user
ansible_ec2=ec2-user
ansible_ssh_private_key_file=~/prod-keys/{{private_key}}
bastion_hostname={{bastion_hostname}}
pem_file_name={{private_key}}